sys:FossilFuels a probs:Object,
        probs:ReferenceObject ;
    rdfs:label "Fossil fuels" ;
    probs:objectComposedOf sys:Coal,
        sys:NaturalGas,
        sys:Oil ;
    probs:objectName "Fossil fuels" .

sys:HydrogenSynthesis a probs:Process ;
    rdfs:label "Hydrogen synthesis" ;
    probs:consumes sys:FossilFuels ;
    probs:processComposedOf sys:HydrogenSynthesisFromCoal,
        sys:HydrogenSynthesisFromNaturalGas ;
    probs:processName "Hydrogen synthesis" ;
    probs:produces sys:Hydrogen .

sys:HydrogenSynthesisFromCoal a probs:Process ;
    rdfs:label "Hydrogen synthesis from coal" ;
    probs:consumes sys:Coal,
        sys:Water ;
    probs:processName "Hydrogen synthesis from coal" ;
    probs:produces sys:CO2,
        sys:Hydrogen ;
    rec:hasRecipe [ probs:consumes [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:Coal ;
                    rec:quantity 1e+00 ],
                [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:Water ;
                    rec:quantity 1e+00 ] ;
            probs:produces [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:CO2 ;
                    rec:quantity 1e+00 ],
                [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:Hydrogen ;
                    rec:quantity 1e+00 ] ] .

sys:HydrogenSynthesisFromNaturalGas a probs:Process ;
    rdfs:label "Hydrogen synthesis from natural gas" ;
    probs:consumes sys:NaturalGas,
        sys:Water ;
    probs:processName "Hydrogen synthesis from natural gas" ;
    probs:produces sys:CO2,
        sys:Hydrogen ;
    rec:hasRecipe [ probs:consumes [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:NaturalGas ;
                    rec:quantity 1e+00 ],
                [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:Water ;
                    rec:quantity 1e+00 ] ;
            probs:produces [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:CO2 ;
                    rec:quantity 1e+00 ],
                [ rec:metric <http://qudt.org/vocab/quantitykind/Mass> ;
                    rec:object sys:Hydrogen ;
                    rec:quantity 1e+00 ] ] .

